{
  "name": "ngrok-docs-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "mint build",
    "dev": "mint dev --port 3333",
    "fmt:check": "biome check . --diagnostic-level=error && prettier --check .",
    "fmt": "biome check . --write --diagnostic-level=error --organize-imports-enabled=true && prettier --write .",
    "verify-redirects": "node snippets/custom-scripts/check-redirect-conflicts.js",
    "test-links": "mint broken-links",
    "test-dev": "mint dev & pid=$!; sleep 60; kill $pid"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@ngrok/mantle": "^0.52.9",
    "@types/react": "^19.1.13",
    "biome": "^0.3.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "mint": "^4.2.131",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-markdown": "^10.1.0"
  }
}
