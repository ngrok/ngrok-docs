{
  "name": "ngrok-doc",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "docusaurus": "docusaurus",
    "fmt:check": "biome check . --diagnostic-level=error && prettier --check .",
    "fmt": "biome check . --write --diagnostic-level=error --organize-imports-enabled=true && prettier --write .",
    "dev": "pnpm run start",
    "clean-dev": "npm cache clean --force && npm cache verify && pnpm run dev",
    "start": "docusaurus start --host 0.0.0.0",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=5120 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "typecheck": "tsc",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build:validate-codeblocks": "tsc -p src/validate-codeblocks/tsconfig.json",
    "validate-codeblocks": "pnpm run build:validate-codeblocks && node dist/validate-codeblocks/index.js",
    "validate-codeblocks:warnings": "pnpm run build:validate-codeblocks && node dist/validate-codeblocks/index.js --warnings",
    "validate-codeblocks:quiet": "pnpm run build:validate-codeblocks && node dist/validate-codeblocks/index.js --quiet",
    "test:validate-codeblocks": "jest --testPathPattern=src/validate-codeblocks"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "1.19.0",
    "@docsearch/react": "3.9.0",
    "@docusaurus/core": "3.7.0",
    "@docusaurus/plugin-client-redirects": "3.7.0",
    "@docusaurus/plugin-google-analytics": "3.7.0",
    "@docusaurus/preset-classic": "3.7.0",
    "@docusaurus/theme-common": "3.7.0",
    "@docusaurus/theme-mermaid": "3.7.0",
    "@docusaurus/theme-search-algolia": "3.7.0",
    "@docusaurus/utils": "3.7.0",
    "@loadable/component": "5.16.4",
    "@mdx-js/react": "3.1.0",
    "@ngrok/mantle": "0.27.2",
    "@phosphor-icons/react": "2.1.7",
    "@stackql/docusaurus-plugin-hubspot": "1.1.0",
    "algoliasearch": "5.23.4",
    "clsx": "2.1.1",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
    "dotenv": "16.5.0",
    "mermaid": "11.6.0",
    "prismjs": "1.30.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hubspot-form": "1.3.7",
    "react-markdown": "10.1.0",
    "react-player": "2.16.0",
    "yaml": "2.7.1",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@docusaurus/module-type-aliases": "3.7.0",
    "@docusaurus/plugin-content-docs": "3.7.0",
    "@docusaurus/theme-classic": "3.7.0",
    "@docusaurus/tsconfig": "3.7.0",
    "@docusaurus/types": "3.7.0",
    "@types/jest": "^29.5.0",
    "@types/loadable__component": "5.13.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "22.15.2",
    "@types/react": "18.3.20",
    "@types/react-dom": "18.3.6",
    "autoprefixer": "10.4.21",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "globby": "^11.1.0",
    "jest": "^29.5.0",
    "markdown-it": "^14.1.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "3.4.17",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "cookie": ">=1.0.2",
      "esbuild": ">=0.25.2"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "core-js-pure"
    ]
  }
}
