{
  "name": "ngrok-doc",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "docusaurus": "docusaurus",
    "fmt:check": "biome check . --diagnostic-level=error && prettier --check .",
    "fmt": "biome check . --write --diagnostic-level=error --organize-imports-enabled=true && prettier --write .",
    "dev": "pnpm run start",
    "clean-dev": "npm cache clean --force && npm cache verify && pnpm run dev",
    "start": "docusaurus start --host 0.0.0.0",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=5120 docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "typecheck": "tsc",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "validate-codeblocks": "tsx src/validate-codeblocks/index.ts",
    "validate-codeblocks:warnings": "tsx src/validate-codeblocks/index.ts --warnings",
    "validate-codeblocks:quiet": "tsx src/validate-codeblocks/index.ts --quiet",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clear-cache": "rm -rf .docusaurus node_modules/.cache",
    "clear-cache-win": "rmdir /s /q \".docusaurus\" && rmdir /s /q \"node_modules\\.cache\""
  },
  "dependencies": {
    "@algolia/autocomplete-core": "1.19.2",
    "@docsearch/react": "3.9.0",
    "@docusaurus/core": "3.8.1",
    "@docusaurus/plugin-client-redirects": "3.8.1",
    "@docusaurus/plugin-google-analytics": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/theme-common": "3.8.1",
    "@docusaurus/theme-mermaid": "3.8.1",
    "@docusaurus/theme-search-algolia": "3.8.1",
    "@docusaurus/utils": "3.8.1",
    "@loadable/component": "5.16.7",
    "@mdx-js/react": "3.1.0",
    "@ngrok/mantle": "0.40.0",
    "@phosphor-icons/react": "2.1.10",
    "@stackql/docusaurus-plugin-hubspot": "1.1.0",
    "@types/capitalize": "2.0.2",
    "algoliasearch": "5.35.0",
    "capitalize": "2.0.4",
    "clsx": "2.1.1",
    "concurrently": "9.2.0",
    "cross-env": "10.0.0",
    "dotenv": "17.2.1",
    "mermaid": "11.9.0",
    "prismjs": "1.30.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hubspot-form": "1.3.7",
    "react-markdown": "10.1.0",
    "react-player": "2.16.1",
    "unist-util-visit": "5.0.0",
    "yaml": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/plugin-content-docs": "3.8.1",
    "@docusaurus/theme-classic": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@types/loadable__component": "5.13.10",
    "@types/markdown-it": "14.1.2",
    "@types/node": "22.17.1",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "commander": "14.0.0",
    "markdown-it": "14.1.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "3.4.17",
    "tinyglobby": "0.2.14",
    "tsx": "4.20.3",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "overrides": {
      "cookie": ">=1.0.2",
      "esbuild": ">=0.25.8"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "core-js-pure",
      "esbuild"
    ]
  }
}
